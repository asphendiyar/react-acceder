{
  "name": "react-acceder",
  "version": "1.0.7",
  "description": "React Context driven role-access package",
  "main": "dist/index",
  "module": "dist/index",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "react-scripts start",
    "build": "tsc",
    "build-babel": "babel src/lib --out-dir dist --copy-files",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react-access",
    "react permissions",
    "rbac"
  ],
  "author": "Nurbek Zhussip",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nurbekzhussip/react-acceder/issues"
  },
  "homepage": "https://github.com/nurbekzhussip/react-acceder#readme",
  "files": [
    "dist",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurbekzhussip/react-acceder.git"
  },
  "peerDependencies": {
    "react": ">=0.14.9"
  },
  "dependencies": {
    "create-react-context": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@types/react": "^18.0.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.3"
  }
}
